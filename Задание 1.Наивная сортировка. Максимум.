#include <iostream>

using namespace std;

int main()
{
    int n, maxn, max=0;
    cin>>n;
    int *mas=new int [n];
    for (int i=0; i<n; i++) cin>>*(mas+i); //Заполняю массив
    
    for (int j=0; j<n; j++){    //цикл для сортировки вставки.
    for (int i=0+j; i<n; i++){  //внутренний цикл для нахождения...
    if (*(mas+i)>=max){          //...максимального элемента и его номера.
        max=*(mas+i); maxn=i;
    }
    }
    
        *(mas+maxn)=*(mas+j);    //перемещаю наверх макс.элемент
        *(mas+j)=max; 
        max=0; 
        cout<<*(mas+j)<<" ";
}
    return 0;
}
