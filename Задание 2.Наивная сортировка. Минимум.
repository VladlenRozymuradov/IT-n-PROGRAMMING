#include <iostream>

using namespace std;

int main()
{
    int n, minn, min;
    cin>>n;
    int *mas=new int [n];
    for (int i=0; i<n; i++) cin>>*(mas+i); //Заполняю массив
    for (int j=0; j<n; j++){    //цикл для сортировки вставки.
    min=mas[j];
    for (int i=0+j; i<n; i++){  //внутренний цикл для нахождения...
    if (*(mas+i)<=min){          //...min элемента и его номера.
        min=*(mas+i); minn=i;
    }
    }
    
        *(mas+minn)=*(mas+j);    //перемещаю наверх min.элемент
        *(mas+j)=min; 
        cout<<*(mas+j)<<" ";
}
    return 0;
}

